  "categories": {
    "localization": ["Cytoplasmic", "CytoMembrane", "Periplasmic", "OuterMembrane", "Extracellular", "Unknown"],
    "pangenome": ["Unknown", "Accessory", "Core"]
  },
  "tracks": [
    { "id": "order", "name": "Gene Order", "field": "ID", "type": "sequential", "enabled": true },
    { "id": "strand", "name": "Gene Direction", "field": "STRAND", "type": "binary", "enabled": true },
    { "id": "conservation", "name": "Pangenome Conservation", "field": "CONS_FRAC", "type": "sequential", "enabled": true },
    { "id": "pan_category", "name": "Core/Accessory", "field": "PAN_CAT", "type": "categorical", "enabled": true, "categories": "pangenome" },
    { "id": "avg_cons", "name": "Function Consensus (avg)", "field": "AVG_CONS", "type": "consistency", "enabled": true },
    { "id": "rast_cons", "name": "RAST Consistency", "field": "RAST_CONS", "type": "consistency", "enabled": false },
    { "id": "ko_cons", "name": "KO Consistency", "field": "KO_CONS", "type": "consistency", "enabled": false },
    { "id": "go_cons", "name": "GO Consistency", "field": "GO_CONS", "type": "consistency", "enabled": false },
    { "id": "ec_cons", "name": "EC Consistency", "field": "EC_CONS", "type": "consistency", "enabled": false },
    { "id": "bakta_cons", "name": "Bakta Consistency", "field": "BAKTA_CONS", "type": "consistency", "enabled": false },
    { "id": "specificity", "name": "Annotation Specificity", "field": "SPECIFICITY", "type": "consistency", "enabled": false },
    { "id": "n_ko", "name": "# KEGG Terms", "field": "N_KO", "type": "sequential", "enabled": false },
    { "id": "n_cog", "name": "# COG Terms", "field": "N_COG", "type": "sequential", "enabled": false },
    { "id": "n_pfam", "name": "# Pfam Terms", "field": "N_PFAM", "type": "sequential", "enabled": false },
    { "id": "n_go", "name": "# GO Terms", "field": "N_GO", "type": "sequential", "enabled": false },
    { "id": "localization", "name": "Subcellular Localization", "field": "LOC", "type": "categorical", "enabled": false, "categories": "localization" },
    { "id": "has_name", "name": "Has Gene Name", "field": "HAS_NAME", "type": "binary", "enabled": false },
    { "id": "n_ec", "name": "# EC Numbers", "field": "N_EC", "type": "sequential", "enabled": false },
    { "id": "cluster_size", "name": "Cluster Size", "field": "CLUSTER_SIZE", "type": "sequential", "enabled": false },
    { "id": "prot_len", "name": "Protein Length", "field": "PROT_LEN", "type": "sequential", "enabled": false },
    { "id": "rich_flux", "name": "Flux (rich media)", "field": "RICH_FLUX", "type": "sequential", "enabled": false },
    { "id": "min_flux", "name": "Flux (minimal media)", "field": "MIN_FLUX", "type": "sequential", "enabled": false },
    { "id": "rich_class", "name": "Flux Class (rich)", "field": "RICH_CLASS", "type": "categorical", "enabled": false, "categories": ["essential", "variable", "blocked"] },
    { "id": "min_class", "name": "Flux Class (minimal)", "field": "MIN_CLASS", "type": "categorical", "enabled": false, "categories": ["essential", "variable", "blocked"] },
    { "id": "essentiality", "name": "Gene Essentiality", "field": "ESSENTIALITY", "type": "sequential", "enabled": false },
    { "id": "n_phenotypes", "name": "# Phenotypes", "field": "N_PHENOTYPES", "type": "sequential", "enabled": false },
    { "id": "n_fitness", "name": "# Fitness Scores", "field": "N_FITNESS", "type": "sequential", "enabled": false },
    { "id": "fitness_avg", "name": "Avg Fitness Score", "field": "FITNESS_AVG", "type": "diverging", "enabled": false }
