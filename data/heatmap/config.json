  "placeholder_tracks": [
    { "id": "neighborhood", "name": "Neighborhood Conservation*", "pending": true },
    { "id": "n_modules", "name": "KEGG Module Hits*", "pending": true },
    { "id": "ec_map_cons", "name": "EC Mapped Consistency*", "pending": true }
  ],
  "sort_presets": [
    { "id": "genome", "name": "Genome Order" },
    { "id": "conservation", "name": "Conservation", "field": "CONS_FRAC", "direction": "desc" },
    { "id": "consistency", "name": "Consistency", "field": "AVG_CONS", "direction": "desc", "na_last": true },
    { "id": "annotation_quality", "name": "Annotation Depth", "composite": ["N_KO", "N_COG", "N_PFAM", "N_GO", "FUNC"], "direction": "desc" },
    { "id": "pan_status", "name": "Pangenome Status", "field": "PAN_CAT", "direction": "desc", "tiebreak": "CONS_FRAC" },
    { "id": "low_consistency", "name": "Lowest Consistency", "field": "AVG_CONS", "direction": "asc", "na_last": true },
    { "id": "strand_blocks", "name": "Strand Blocks", "field": "STRAND", "direction": "desc", "tiebreak": "START" }
  ],
  "analysis_views": [
    { "id": "characterization", "name": "Characterization Targets", "tracks": ["pan_category", "conservation", "avg_cons", "specificity"], "sort": "pan_status", "summary": "Find conserved genes with unknown function" },
    { "id": "annotation_quality", "name": "Annotation Quality", "tracks": ["avg_cons", "specificity"], "sort": "low_consistency", "summary": "Spot annotation conflicts and gaps" },
    { "id": "metabolic", "name": "Metabolic Landscape", "tracks": ["n_ec", "ec_cons", "rich_class", "essentiality"], "sort": null, "summary": "Map enzymatic and metabolic gene activity" },
    { "id": "pangenome", "name": "Pangenome Structure", "tracks": ["conservation", "pan_category", "cluster_size", "strand"], "sort": "conservation", "summary": "Explore pangenome architecture and gene families" },
    { "id": "annotation_depth", "name": "Knowledge Coverage", "tracks": ["has_name", "n_ko", "n_go", "n_pfam", "n_ec"], "sort": "annotation_quality", "summary": "How well-characterized is each gene?" },
    { "id": "consistency_compare", "name": "Consistency Comparison", "tracks": ["rast_cons", "ko_cons", "bakta_cons", "ec_cons", "go_cons"], "sort": "low_consistency", "summary": "Compare consistency across annotation sources" }
  ],
  "default_tracks": ["order", "strand", "conservation", "pan_category", "avg_cons"],
  "cluster_color_options": [
    { "value": "pan_category", "label": "Core/Accessory" },
    { "value": "conservation", "label": "Conservation" },
    { "value": "avg_cons", "label": "Avg Consistency" },
    { "value": "localization", "label": "Localization" },
    { "value": "n_ko", "label": "# KEGG Terms" },
    { "value": "essentiality", "label": "Essentiality" },
    { "value": "cluster_size", "label": "Cluster Size" },
    { "value": "specificity", "label": "Specificity" }
  ]
